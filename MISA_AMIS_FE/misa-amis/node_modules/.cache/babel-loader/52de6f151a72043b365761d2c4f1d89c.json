lose {\n  position: absolute;\n  top: 0;\n  right: 0;\n}\n\n.dialog-close .dialog-close-btn {\n  padding: 10px;\n  cursor: pointer;\n}\n\n.dialog-close div:hover {\n  background-color: #ccc;\n}\n\n.dialog-body {\n  border-bottom: 1px solid #ddd;\n}\n.dialog-main-content {\n  display: flex;\n  align-items: center;\n}\n.dialog-message {\n  overflow: auto;\n  max-height: 400px;\n  margin-bottom: 32px;\n  padding-left: 16px;\n  padding-top: 12px;\n}\n.dialog-icon {\n  margin-right: 20px;\n}\n.dialog-message span {\n  display: block;\n}\n\n.dialog-footer {\n  margin-top: 20px;\n}\n\n.dialog-footer {\n  display: flex;\n  justify-content: space-between;\n}\n.btnCloseModal {\n  background-color: #fff;\n  border: 1px solid #8d9096;\n  cursor: pointer;\n  width: fit-content;\n  color: #000;\n  padding: 6px 16px 6px 16px;\n  border-radius: 3px;\n  transition: all 0.2s ease;\n}\n.btnCloseModal:hover {\n  background-color: #d2d3d6;\n}\n.btnAcceptModal {\n  background-color: #2ca01c;\n  border: 1px solid transparent;\n  cursor: pointer;\n  width: fit-content;\n  color: #fff;\n  padding: 6px 16px 6px 16px;\n  border-radius: 3px;\n  transition: all 0.2s ease;\n}\n.btnAcceptModal:hover {\n  background-color: #35bf22;\n}\n</style>"]},"metadata":{},"sourceType":"module"}