);\n    },\n\n    /**\r\n     * Xuất khẩu \r\n     */\n    exportExcelFile() {\n      document.getElementById(\"loader\").style.display = \"block\";\n\n      try {\n        axios({\n          url: \"https://localhost:44317/api/v1/Employees/EmployeeExcel\",\n          method: \"GET\",\n          responseType: \"blob\"\n        }).then(response => {\n          var fileURL = window.URL.createObjectURL(new Blob([response.data]));\n          var fileLink = document.createElement(\"a\");\n          fileLink.href = fileURL;\n          fileLink.setAttribute(\"download\", \"Danh_sach_nhan_vien.xlsx\");\n          document.body.appendChild(fileLink);\n          fileLink.click();\n          document.getElementById(\"loader\").style.display = \"none\";\n        });\n      } catch (error) {\n        document.getElementById(\"loader\").style.display = \"none\";\n        console.log(error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,eAAP,MAA4B,8CAA5B;AACA,OAAOC,MAAP,MAAmB,oCAAnB;AACA,OAAOC,cAAP,MAA2B,sBAA3B;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,UAAnC,QAAqD,MAArD;AAEA,eAAe;EACbC,UAAU,EAAE;IAAEN,eAAF;IAAmBC,MAAnB;IAA2BC;EAA3B,CADC;;EAEbK,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,IADT;MAELC,OAAO,EAAE,IAFJ;MAGLC,SAAS,EAAE,EAHN;MAILC,QAAQ,EAAE,IAJL;MAKLC,gBAAgB,EAAE,EALb;MAMLC,WAAW,EAAE,IANR;MAOLC,SAAS,EAAE;IAPN,CAAP;EASD,CAZY;;EAabC,OAAO,GAAE;IACP,KAAKC,iBAAL,CAAuB;MAACC,QAAQ,EAAE,EAAX;MAAeC,UAAU,EAAE;IAA3B,CAAvB;EACD,CAfY;;EAgBbC,QAAQ,EAAE,EACR,GAAGd,UAAU,CAAC,CAAC,WAAD,EAAc,gBAAd,CAAD;EADL,CAhBG;EAmBbe,OAAO,EAAE,EACP,GAAGjB,YAAY,CAAC,CAAC,mBAAD,CAAD,CADR;IAEP,GAAGC,UAAU,CAAC,CAAC,mBAAD,CAAD,CAFN;;IAIP;;;;IAIAiB,UAAU,CAACC,IAAD,EAAOC,MAAP,EAAeC,GAAf,EAAmB;MAC3B,KAAKhB,YAAL,GAAoBe,MAApB;MACA,KAAKZ,QAAL,GAAgBW,IAAhB;MACA,KAAKV,gBAAL,GAAwBY,GAAxB;IACD,CAZM;;IAcP;;;;IAIAC,aAAa,CAAClB,IAAD,EAAO;MAClBmB,YAAY,CAAC,KAAKjB,OAAN,CAAZ;MACA,IAAIkB,IAAG,GAAI,IAAX;MACAA,IAAI,CAAClB,OAAL,GAAemB,UAAU,CAAC,YAAY;QACpCD,IAAI,CAACjB,SAAL,GAAiBH,IAAI,CAACsB