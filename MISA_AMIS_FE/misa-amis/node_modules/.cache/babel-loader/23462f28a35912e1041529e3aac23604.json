{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport MPopup from \"@/components/base/popup/MPopup.vue\";\nimport MCheckbox from \"@/components/base/checkbox/MCheckbox.vue\";\nimport MRadioCheckbox from '@/components/base/checkbox/MRadioCheckbox.vue';\nimport MButton from '@/components/base/button/MButton.vue';\nimport { mapMutations, mapActions, mapGetters } from \"vuex\";\nexport default {\n  components: {\n    MPopup,\n    MCheckbox,\n    MRadioCheckbox,\n    MButton\n  },\n  props: ['isHide', 'saveMode', 'employeeSelected'],\n\n  data() {\n    return {\n      employee: {},\n      isCustomer: false,\n      isGroup: false,\n      isShowError: [],\n      errorText: [],\n      isValid: true\n    };\n  },\n\n  computed: { ...mapGetters([, \"organizations\", \"positions\"])\n  },\n\n  mounted() {\n    this.getNewEmployeeCode();\n    this.getOrganizations();\n    this.getPositions();\n  },\n\n  watch: {\n    employeeSelected: function (value) {\n      this.employee = value;\n    },\n    isHide: function (value) {\n      if (!value && this.saveMode == 1) {\n        this.employee.EmployeeCode = this.newEmployeeCode;\n        this.employee.Gender = 1;\n      }\n    }\n  },\n  methods: { ...mapMutations([\"setNewEmployeeCode\", \"setOrganizations\", \"setPositions\"]),\n    ...mapActions([\"getNewEmployeeCode\", \"getOrganizations\", \"getPositions\"]),\n\n    hideDialogDetail() {\n      this.$emit(\"hideDetail\", null, true);\n    },\n\n    onInitialized: function (e) {\n      setTimeout(function () {\n        e.component.focus();\n      }, 0);\n    },\n\n    saveOnClick() {\n      var newGuid = this.$uuid.v4();\n      this.employee.EmployeeID = newGuid;\n      var list = [];\n      list.push(this.employee);\n      this.validateEmployee();\n\n      if (this.isValid) {\n        try {\n          axios.post(\"https://localhost:44317/api/v1/Employees\", list).then(response => {\n            console.log(response);\n          });\n        } catch (error) {\n          console.log(error);\n        }\n      }\n    },\n\n    refillFormOnClick() {\n      this.saveOnClick();\n      this.employee = {};\n      this.getNewEmployeeCode();\n      this.employee.EmployeeCode = this.newEmployeeCode;\n    },\n\n    checkCheckbox(check) {\n      if (check == 1) {\n        if (this.employee.IsCustomer == 1) {\n          this.employee.IsCustomer = 0;\n        } else {\n          this.employee.IsCustomer = 1;\n        }\n      } else if (check == 2) {\n        if (this.employee.IsGroup == 1) {\n          this.employee.IsGroup = 0;\n        } else {\n          this.employee.IsGroup = 1;\n        }\n      }\n    },\n\n    checkRadio(check) {\n      if (check == 1) {\n        this.employee.Gender = 1;\n      } else if (check == 2) {\n        this.employee.Gender = 2;\n      }\n    },\n\n    validateEmployee() {\n      this.isValid = true;\n      this.validateEmployeeCode();\n      this.validateFullName();\n      this.validateOrganization();\n      this.validateDateOfBirth();\n      this.validateDateOfIssuanceCIC();\n      this.validateEmail();\n\n      for (let i = 0; i < this.errorText.length; i++) {\n        if (this.errorText[i]) {\n          this.isValid = false;\n          break;\n        }\n      }\n    },\n\n    validateEmployeeCode() {\n      let el = document.getElementById(\"employeeCodeInput\");\n\n      if (!this.employee.EmployeeCode) {\n        el.classList.add(\"check-border\");\n        this.errorText[0] = \"Mã không được để trống.\";\n      } else {\n        el.classList.remove(\"check-border\");\n        this.errorText[0] = \"\";\n      }\n    },\n\n    validateFullName() {\n      let el = document.getElementById(\"fullNameInput\");\n\n      if (!this.employee.FullName) {\n        el.classList.add(\"check-border\");\n        this.errorText[1] = \"Tên không được để trống.\";\n      } else {\n        el.classList.remove(\"check-border\");\n        this.errorText[1] = \"\";\n      }\n    },\n\n    validateOrganization() {\n      let el = document.getElementById(\"organizationInput\");\n\n      if (!this.employee.OrganizationID) {\n        el.classList.add(\"check-border\");\n        this.errorText[2] = \"Đơn vị không được để trống.\";\n      } else {\n        el.classList.remove(\"check-border\");\n        this.errorText[2] = \"\";\n      }\n    },\n\n    validateEmail() {\n      let el = document.getElementById(\"emailInput\");\n\n      if (this.employee.Email) {\n        var emailFormat = /^([a-zA-Z0-9_\\-.]+)@([a-zA-Z0-9_\\-.]+)\\.([a-zA-Z]{2,5})$/;\n\n        if (!emailFormat.test(this.employee.Email)) {\n          el.classList.add(\"check-border\");\n          this.errorText[5] = \"Email không đúng định dạng.\";\n        } else {\n          el.classList.remove(\"check-border\");\n          this.errorText[5] = \"\";\n        }\n      } else {\n        el.classList.remove(\"check-border\");\n        this.errorText[5] = \"\";\n      }\n    },\n\n    validateDateOfBirth() {\n      let el = document.getElementById(\"dobInput\");\n\n      if (this.employee.DateOfBirth) {\n        let today = new Date();\n\n        if (this.employee.DateOfBirth > today) {\n          el.classList.add(\"check-border\");\n          this.errorText[3] = \"Ngày sinh không được lớn hơn ngày hiện tại.\";\n        } else {\n          el.classList.remove(\"check-border\");\n          this.errorText[3] = \"\";\n        }\n      } else {\n        el.classList.remove(\"check-border\");\n        this.errorText[3] = \"\";\n      }\n    },\n\n    validateDateOfIssuanceCIC() {\n      let el = document.getElementById(\"doiInput\");\n\n      if (this.employee.DateOfIssuanceCIC) {\n        let today = new Date();\n\n        if (this.employee.DateOfIssuanceCIC > today) {\n          el.classList.add(\"check-border\");\n          this.errorText[4] = \"Ngày cấp không được lớn hơn ngày hiện tại\";\n        } else {\n          el.classList.remove(\"check-border\");\n          this.errorText[4] = \"\";\n        }\n      } else {\n        el.classList.remove(\"check-border\");\n        this.errorText[4] = \"\";\n      }\n    },\n\n    /**\r\n     * Lấy mã nhân viên mới\r\n     * Author: VQBao - 11/10/2022\r\n     */\n    async getNewEmployeeCode() {\n      try {\n        await axios.get(\"https://localhost:44317/api/v1/Employees/NewEmployeeCode\").then(response => {\n          this.newEmployeeCode = response.data;\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA0QA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,oCAAnB;AACA,OAAOC,SAAP,MAAsB,0CAAtB;AACA,OAAOC,cAAP,MAA2B,+CAA3B;AACA,OAAOC,OAAP,MAAoB,sCAApB;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,UAAnC,QAAqD,MAArD;AAEA,eAAe;EACbC,UAAU,EAAE;IAAEP,MAAF;IAAUC,SAAV;IAAqBC,cAArB;IAAqCC;EAArC,CADC;EAEbK,KAAK,EAAE,CAAC,QAAD,EAAW,UAAX,EAAuB,kBAAvB,CAFM;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,UAAU,EAAE,KAFP;MAGLC,OAAO,EAAE,KAHJ;MAILC,WAAW,EAAE,EAJR;MAKLC,SAAS,EAAE,EALN;MAMLC,OAAO,EAAE;IANJ,CAAP;EAQD,CAZY;;EAabC,QAAQ,EAAE,EACR,GAAGV,UAAU,CAAC,GAAG,eAAH,EAAoB,WAApB,CAAD;EADL,CAbG;;EAgBbW,OAAO,GAAE;IACP,KAAKC,kBAAL;IACA,KAAKC,gBAAL;IACA,KAAKC,YAAL;EACD,CApBY;;EAqBbC,KAAK,EAAE;IACLC,gBAAgB,EAAE,UAASC,KAAT,EAAe;MAC/B,KAAKb,QAAL,GAAgBa,KAAhB;IACD,CAHI;IAILC,MAAM,EAAE,UAASD,KAAT,EAAe;MACrB,IAAG,CAACA,KAAD,IAAU,KAAKE,QAAL,IAAiB,CAA9B,EAAgC;QAC9B,KAAKf,QAAL,CAAcgB,YAAd,GAA6B,KAAKC,eAAlC;QACA,KAAKjB,QAAL,CAAckB,MAAd,GAAuB,CAAvB;MACF;IACD;EATI,CArBM;EAgCbC,OAAO,EAAE,EACP,GAAGzB,YAAY,CAAC,CAAC,oBAAD,EAAuB,kBAAvB,EAA2C,cAA3C,CAAD,CADR;IAEP,GAAGC,UAAU,CAAC,CAAC,oBAAD,EAAuB,kBAAvB,EAA2C,cAA3C,CAAD,CAFN;;IAIPyB,gBAAgB,GAAE;MAChB,KAAKC,KAAL,CAAW,YAAX,EAAyB,IAAzB,EAA+B,IAA/B;IACD,CANM;;IAOPC,aAAa,EAAE,UAAUC,CAAV,EAAa;MAC1BC,UAAU,CAAC,YAAY;QACrBD,CAAC,CAACE,SAAF,CAAYC,KAAZ;MACD,CAFS,EAEP,CAFO,CAAV;IAGD,CAXM;;IAYPC,WAAW,GAAE;MACX,IAAIC,OAAM,GAAI,KAAKC,KAAL,CAAWC,EAAX,EAAd;MACA,KAAK9B,QAAL,CAAc+B,UAAd,GAA2BH,OAA3B;MACA,IAAII,IAAG,GAAI,EAAX;MACAA,IAAI,CAACC,IAAL,C